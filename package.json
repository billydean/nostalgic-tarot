{
  "name": "nostalgic-tarot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.0",
    "sqlite3": "^5.1.6"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "concurrently \"yarn run server:dev\" \"yarn run client:dev\"",
    "client:dev": "vite",
    "server:dev": "nodemon api/index.js",
    "server:build": "babel --verbose --out-dir dist --only 'api/index.js,api/lib/' src",
    "client:build": "vite build",
    "build": "yarn run server:build && yarn run client:build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "prettier --check . && eslint . && stylelint \"**/*.css\"",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.7",
    "@vitejs/plugin-react": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.22",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "vite": "^2.5.10"
  }
}
